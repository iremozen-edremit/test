#!/home/irem/PycharmProjects/test/venv/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'Curtains==0.3.1','console_scripts','curt'
__requires__ = 'Curtains==0.3.1'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('Curtains==0.3.1', 'console_scripts', 'curt')()
    )
